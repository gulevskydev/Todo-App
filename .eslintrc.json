{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": ["airbnb-base", "plugin:vue/recommended"],
  "rules": {
    "quotes": [2, "single", { "avoidEscape": true }],
    "max-len": "off",
    "object-shorthand": ["error", "never"]
  }
}
